<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        #tes {
            border: 1px solid;
        }
    </style>
</head>
<body>
    <%- include(`./partials/navbar`) %> 
    <h1>List Label Detail</h1>
    <table border="1">
        <thead>
            <th>name</th>
            <th>since</th>
            <th>city</th>
            <th>AVG</th>
            <th>MIN</th>
            <th>MAX</th>
        </thead>
        <tbody >
            <% data.forEach(el => { %>
                <tr>
                    <td><%= el.name %> </td>
                    <td><%= el.toDate %> </td>
                    <td><%= el.city %> </td>
                    <td><%= el.avarageDuration %> </td>
                    <td><%= el.minDuration %> </td>
                    <td><%= el.maxDuration %> </td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>